msgid "0 to 255 with"
msgstr "0-tól 255-ig terjed"

#: activities/binary_bulb/resource/tutorial5.qml:29
msgctxt "tutorial5|"
msgid "0 to 255 with"
msgstr "0-tól 255-ig terjed"


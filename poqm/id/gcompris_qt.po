msgid "%1%"
msgstr "%1%"

#: activities/solar_system/QuizScreen.qml:274
#, qt-format
msgctxt "QuizScreen|"
msgid "Your final score is: <font color=\"#3bb0de\">%1%</font>.<br><br>%2"

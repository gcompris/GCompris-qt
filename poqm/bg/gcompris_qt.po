msgid "a|b|c|d|e|f|g|h|i|j|k|l|m|n|o|p|q|r|s|t|u|v|w|x|y|z"
msgstr "а|б|в|г|д|е|ж|з|и|й|к|л|м|н|о|п|р|с|т|у|ф|х|ц|ч|ш|щ|ъ|ь|ю|я"

#: activities/ordering_alphabets/resource/4/Data.qml:13
msgctxt "Data|"
msgid "Descending order, 5 random letters."
msgstr "Низходящ ред, 5 произволни букви."

#: activities/ordering_alphabets/resource/5/Data.qml:13
msgctxt "Data|"
msgid "Ascending order, 8 random letters."
msgstr "Възходящ ред, 8 произволни букви."

#: activities/ordering_alphabets/resource/6/Data.qml:13
msgctxt "Data|"
msgid "Descending order, 8 random letters."
msgstr "Низходящ ред, 8 произволни букви."

#. Activity title
#: activities/ordering_chronology/ActivityInfo.qml:21
msgctxt "ActivityInfo|"
msgid "Ordering chronology"
msgstr "Подреждане на хронология"

#. Help title
#: activities/ordering_chronology/ActivityInfo.qml:23
msgctxt "ActivityInfo|"
msgid "Arrange the given events in their chronological order."

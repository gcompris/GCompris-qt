msgstr "Urdin gehiegi"

#: activities/color_mix/ColorMix.qml:216
msgctxt "ColorMix|"
msgid "Not enough magenta"
msgstr "Ez dago magenta nahikorik"

#: activities/color_mix/ColorMix.qml:218
msgctxt "ColorMix|"
msgid "Too much magenta"
msgstr "Magenta gehiegi"

#: activities/color_mix/ColorMix.qml:221
msgctxt "ColorMix|"
msgid "Not enough yellow"
msgstr "Ez dago hori nahikorik"

#: activities/color_mix/ColorMix.qml:223
msgctxt "ColorMix|"
msgid "Too much yellow"
msgstr "Hori gehiegi"

#: activities/color_mix/ColorMix.qml:226
msgctxt "ColorMix|"

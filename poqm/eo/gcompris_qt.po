msgctxt "tutorial4|"
msgid "0 to 255 with"
msgstr "0 ĝis 255 kun"

#: activities/binary_bulb/resource/tutorial5.qml:29
msgctxt "tutorial5|"
msgid "0 to 255 with"
msgstr "0 ĝis 255 kun"


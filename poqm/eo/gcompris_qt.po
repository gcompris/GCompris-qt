msgstr "Vi manĝis malĝustan nombron."

#: activities/gnumch-equality/Warning.qml:60
msgctxt "Warning|"
msgid "1 is not a prime number."
msgstr "1 ne estas primo."

#. the first argument corresponds to a number and the second argument corresponds to its divisors formatted as an unordered html format (<ul><li>number 1</li>...</ul>)
#: activities/gnumch-equality/Warning.qml:75
#, qt-format
msgctxt "Warning|"
msgid "%1 is a non-prime number, its divisors are: %2"
msgstr "%1 ne estas primo, ĝiaj divizoroj estas: %2"

#. %1 is the number to find, %2 and %3 are two multiples of the number, %4 is a wrong number and %5 is the number to find.
#: activities/gnumch-equality/Warning.qml:79
#, qt-format
msgctxt "Warning|"
msgid "Multiples of %1 include %2 or %3 but %4 is not a multiple of %5."
msgstr "Obloj de %1 inkluzivas %2 aŭ %3 sed %4 ne estas oblo de %5."

#. the first argument corresponds to a number and the second argument corresponds to its divisors formatted as an unordered html format (<ul><li>number 1</li>...</ul>
#: activities/gnumch-equality/Warning.qml:94
#, qt-format
msgctxt "Warning|"
msgid "Divisors of %1 are: %2"
msgstr "Divizoroj de %1 estas: %2"

#: activities/gnumch-equality/Warning.qml:124
msgctxt "Warning|"
msgid "Press \"Return\" or click on me to continue."
msgstr "Premu \"Reveno\" aŭ alklaku min por daŭrigi."

#: activities/gnumch-equality/WarnMonster.qml:41
